{
  "name": "Groups Tabs Commands Shortcuts",
  "version": "0.1",
  "description": "A list of groups-tabs commands to be bind to shortcuts",
  "manifest_version": 3,
  "icons": {
    "128": "assets/icon-128.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "new-tab-in-group": {
      "description": "Creates new tab in current tab group"
    },
    "collapse-all-groups": {
      "description": "Collapse all groups"
    },
    "expand-all-groups": {
      "description": "Expand all groups"
    }
  },
  "permissions": [
    "tabGroups"
  ]
}
