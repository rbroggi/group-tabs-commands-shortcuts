{
  "name": "Groups Tabs Commands Shortcuts",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "new-tab-in-group": {
      "description": "Creates new tab in current tab group"
    },
    "collapse-all-groups": {
      "description": "Collapse all groups"
    },
    "expand-all-groups": {
      "description": "Expand all groups"
    }
  },
  "permissions": [
    "tabs",
    "tabGroups"
  ]
}
